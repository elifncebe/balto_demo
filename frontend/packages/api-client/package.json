{
  "name": "@logistics/api-client",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "generate": "openapi-generator-cli generate -i http://localhost:8080/v3/api-docs -g typescript-axios -o src/generated --additional-properties=supportsES6=true,npmName=@logistics/api-client,withSeparateModelsAndApi=true,modelPackage=models,apiPackage=api"
  },
  "dependencies": {
    "@logistics/utils": "*",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.54.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}